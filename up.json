{
  "name": "middlewarekam",
  "profile": "default",
  "regions": [
    "us-east-1"
  ],
  "lambda": {
    "memory": 1024,
    "runtime": "python3.7",
    "warm": true,
    "warm_count": 10,
    "warm_rate": "30m",
    "policy": [
      {
        "Effect": "Allow",
        "Resource": "*",
        "Action": [
          "s3:PutObject",
          "s3:GetObject",
          "s3:DeleteObject"
        ]
      },
      {
        "Sid": "AllowCloudFrontUpdateDistribution",
        "Effect": "Allow",
        "Action": [
          "cloudfront:updateDistribution"
        ],
        "Resource": [
          "*"
        ]
      }
    ]
  },
  "cors": {
    "enable": true,
    "allowed_methods": ["HEAD", "GET", "POST", "PUT", "PATCH", "DELETE"],
    "allowed_headers": ["*"],
    "allow_credentials": true
  }
}
